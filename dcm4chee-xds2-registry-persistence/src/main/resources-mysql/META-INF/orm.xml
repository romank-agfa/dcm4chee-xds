<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
	version="2.0">

	<package>org.dcm4chee.xds2.persistence</package>

	<access>FIELD</access>

	<entity class="Description">
		<table name="description" />
		<attributes>
			<id name="pk">
				<column name="pk" />
				<generated-value strategy="IDENTITY" />
			</id>
		</attributes>
	</entity>
	<entity class="Identifiable">
		<table name="identifiable" />
		<inheritance strategy="SINGLE_TABLE" />
	</entity>
	<entity class="Name">
		<table name="name" />
		<attributes>
			<id name="pk">
				<column name="pk" />
				<generated-value strategy="IDENTITY" />
			</id>
		</attributes>
	</entity>
	<entity class="Slot">
		<table name="slot" />
		<attributes>
			<id name="pk">
				<column name="pk" />
				<generated-value strategy="IDENTITY" />
			</id>
			<basic name="value" optional="false">
				<column name="value" column-definition="varchar(255)" />
			</basic>
		</attributes>
	</entity>
	<entity class="XADIssuer">
		<table name="xad_issuer" />
		<attributes>
			<id name="pk">
				<column name="pk" />
				<generated-value strategy="IDENTITY" />
			</id>
		</attributes>
	</entity>
	<entity class="XADPatient">
		<table name="xad_patient" />
		<attributes>
			<id name="pk">
				<column name="pk" />
				<generated-value strategy="IDENTITY" />
			</id>
		</attributes>
	</entity>
	<entity class="XDSCode">
		<table name="xds_code" />
		<attributes>
			<id name="pk">
				<column name="pk" />
				<generated-value strategy="IDENTITY" />
			</id>
			<basic name="codeMeaning" optional="false">
				<column name="meaning" column-definition="varchar(255)" />
			</basic>
		</attributes>
	</entity>

</entity-mappings>
